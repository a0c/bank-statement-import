    <record id="bank_stmt_import_csv_swedbank_en" model="account.bank.statement.import.csv.format">
      <field name="name">Swedbank EN</field>
      <field name="account_number">Client account</field>
      <field name="currency_code">Currency</field>
      <field name="date">Date</field>
      <field name="payee">Beneficiary/Payer</field>
      <field name="payee_account_number"></field>
      <field name="memo">Details</field>
      <field name="drcr">Debit/Credit</field>
      <field name="amount">Amount</field>
      <field name="ref">Reference number</field>
      <field name="archive_id">Transfer reference</field>
      <field name="tx_type">Transaction type</field>
      <field name="tx_type_balance_start">AS</field>
      <field name="tx_type_balance_end">LS</field>
      <field name="tx_type_turnover">K2</field>
      <field name="doc_nr">Document number</field>
      <field name="match_condition">csv_header[0] == self.account_number</field>
      <field name="date_format">%d-%m-%Y</field>
    </record>
